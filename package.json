{
  "name": "modulebuilder3000",
  "version": "0.0.1",
  "description": "My super awesome module!",
  "keywords": ["my", "module"],
  "main": "src/index.js",
  "repository": "https://github.com/ezekielaquino/ModuleBuilder3000",
  "author": "Ezekiel Aquino <ezekielaquino@gmail.com>",
  "scripts": {
    "microbundle": "cd src && microbundle watch -o ../dist",
    "microbundle:build": "cd src && microbundle build -o ../dist",
    "parcel": "cd demo/src && parcel index.html --out-dir ../",
    "parcel:build": "cd demo/src && parcel build index.html --out-dir ../",
    "start": "yarn microbundle & yarn parcel",
    "build": "yarn microbundle:build && yarn parcel:build"
  },
  "devDependencies": {
    "parcel": "^1.9.7",
    "microbundle": "^0.6.0",
    "stylus": "^0.54.5"
  },
  "main": "../dist/index.js",
  "umd:main": "../dist/index.umd.js",
  "module": "../dist/index.m.js",
  "source": "MyModule.js"
}
